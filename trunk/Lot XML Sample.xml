<!-- Assume a line from the last point back to the first -->
<!-- Both spaces and clear locations can be defined -->

<lot width="100" height="150">
	<space>
		<point x="10" y="15">
		<point x="12" y="25">
		<point x="11" y="35">
	</space>
	<clear>
		<point x="10" y="15">
		<point x="12" y="25">
		<point x="11" y="35">
	</clear>
	<space>
		<point x="10" y="15">
		<point x="12" y="25">
		<point x="11" y="35">
	</space>
</lot>